{
  "name": "Google Sheets",
  "version": "1.0.0",
  "platformVersion": "1",
  "triggers": [
    {
      "key": "newSpreadsheetRow",
      "name": "New spreadsheet row",
      "display": {
        "label": "New spreadsheet row",
        "description": "4",
        "instructions": ""
      },
      "operation": {
        "type": "polling",
        "inputFields": [
          {
            "key": "spreadsheet",
            "label": "Spreadsheet",
            "helpText": "",
            "type": "string",
            "required": true,
            "placeholder": "Enter spreadsheet name",
            "choices": [
              {
                "value": "demo_sheet_1",
                "label": "Demo Sheet 1",
                "sample": "demo_sheet_1"
              },
              {
                "value": "demo_sheet_2",
                "label": "Demo Sheet 2",
                "sample": "demo_sheet_2"
              }
            ]
          },
          {
            "key": "worksheet",
            "label": "Worksheet",
            "helpText": "",
            "type": "string",
            "required": true,
            "placeholder": "Enter worksheet name",
            "choices": [
              {
                "value": "demo_worksheet_1",
                "label": "Demo Worksheet 1",
                "sample": "demo_worksheet_1"
              },
              {
                "value": "demo_worksheet_2",
                "label": "Demo Worksheet 2",
                "sample": "demo_worksheet_2"
              },
              {
                "value": "demo_worksheet_3",
                "label": "Demo Worksheet 3",
                "sample": "demo_worksheet_3"
              }
            ]
          }
        ],
        "outputFields": [
          {
            "key": "newRowColumns",
            "type": "string",
            "list": true
          }
        ],
        "sample": {
          "newRowColumns": [
            "Column A data",
            "Column B data",
            "Column C data",
            "Column D data"
          ]
        }
      }
    }
  ],
  "authentication": {
    "type": "oauth2",
    "test": {
      "method": "GET",
      "url": "https://www.googleapis.com/oauth2/v3/userinfo",
      "headers": {
        "Authorization": "Bearer {{access_token}}"
      }
    },
    "fields": [
      {
        "key": "access_token",
        "type": "string",
        "required": true,
        "computed": true
      },
      {
        "key": "token_type",
        "type": "string",
        "required": true,
        "computed": true
      },
      {
        "key": "scope",
        "type": "string",
        "required": true,
        "computed": true
      },
      {
        "key": "expires_in",
        "type": "number",
        "required": true,
        "computed": true
      }
    ],
    "oauth2Config": {
      "authorizeUrl": {
        "method": "GET",
        "url": "https://accounts.google.com/o/oauth2/v2/auth?prompt=consent&response_type=token&client_id=676778012745-q5lg8up8nq94qet4fjhs3ftvpgd4nalv.apps.googleusercontent.com&scope=https://www.googleapis.com/auth/spreadsheets+https://www.googleapis.com/auth/userinfo.email"
      },
      "getAccessToken": {
        "method": "POST",
        "url": ""
      }
    }
  },
  "icon": "data:image/png;base64,"
}
